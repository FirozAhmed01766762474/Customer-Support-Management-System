@model XFLCSMS.Models.Branch.BranchView;


<!-- Main content -->
<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- left column -->
            <div class="col-md-6">
                <!-- general form elements -->
                <div class="card card-primary">
                    <!-- /.card-header -->
                    <!-- form start -->
                    <form asp-controller="Admin" asp-action="UpdateBranch" method="post">
                        <div class="card-body">
                            <div class="form-group">
                                <label for="exampleInputEmail1">BranchId</label>
                                <input type="text" asp-for="@Model.BranchId" value="@Model.BranchId" class="form-control" id="exampleInputEmail1" placeholder="BranchID" readonly>
                            </div>

                            <div class="form-group">
                                <label for="exampleInputEmail1">BranchName</label>
                                <input type="text" asp-for="@Model.BranchName" value="@Model.BranchName" class="form-control" id="exampleInputEmail1" placeholder="BranchName">
                            </div>

                            <div class="form-group">
                                <label asp-for="@Model.BrokerageHouseName" class="control-label"></label>
                                <input type="text" class="form-control" asp-for="@Model.BrokerageHouseName" value="@Model.BrokerageHouseName" id="exampleInputEmail1" placeholder="BrokerageHouseName" readonly>  
                                            
                                          
                            </div>

                            <div class="form-group">
                                <input type="submit" value="Update User" class="btn btn-primary" />
                            </div>



                        </div>
                        <!-- /.card-body -->
                    </form>
                </div>
                <!-- /.card -->
            </div>
            <!--/.col (left) -->
            <!-- right column -->
            <!--/.col (right) -->
        </div>
        <!-- /.row -->
        <!-- /.card-body -->

    </div>



    <!-- /.container-fluid -->
</section>
            <!-- /.content -->
       
<script type="text/javascript">
    // Define a function named preventback
    function preventback() {
        // Forward the browser's history by one step, effectively preventing the user from going back
        window.history.forward();
    }

    // Set a timeout to call the preventback function immediately after the script is executed
    setTimeout("preventback()", 0);

    // When the window loads, assign a null function to the onload event to effectively do nothing
    window.onload = function () { null };
</script>
