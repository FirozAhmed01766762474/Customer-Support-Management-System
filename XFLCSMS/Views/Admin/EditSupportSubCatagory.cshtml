@model XFLCSMS.Models.Support.SupportSubCatagory;

<!-- Main content -->
<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- left column -->
            <div class="col-md-6">
                <!-- general form elements -->
                <div class="card card-primary">
                    <!-- /.card-header -->
                    <!-- form start -->
                    <form asp-controller="Admin" asp-action="UpdateSupportSubCatagory" method="post">
                        <div class="card-body">
                            <div class="form-group">
                                <label for="exampleInputEmail1">SupportSubCatagoryId</label>
                                <input type="text" asp-for="@Model.SupportSubCatagoryId" value="@Model.SupportSubCatagoryId" class="form-control" id="exampleInputEmail1" placeholder="SupportSubCatagoryId" readonly>
                            </div>

                            <div class="form-group">
                                <label for="exampleInputEmail1">SupportSubCatagoryName</label>
                                <input type="text" asp-for="@Model.SubCatagory" value="@Model.SubCatagory" class="form-control" id="exampleInputEmail1" placeholder="SupportSubCatagoryName">
                            </div>



                            <div class="form-group">
                                <input type="submit" value="Update" class="btn btn-primary" />
                            </div>



                        </div>
                        <!-- /.card-body -->
                    </form>
                </div>
                <!-- /.card -->
            </div>
            <!--/.col (left) -->
            <!-- right column -->
            <!--/.col (right) -->
        </div>
        <!-- /.row -->
        <!-- /.card-body -->

    </div>



    <!-- /.container-fluid -->
</section>
    
<script type="text/javascript">
    // Define a function named preventback
    function preventback() {
        // Forward the browser's history by one step, effectively preventing the user from going back
        window.history.forward();
    }

    // Set a timeout to call the preventback function immediately after the script is executed
    setTimeout("preventback()", 0);

    // When the window loads, assign a null function to the onload event to effectively do nothing
    window.onload = function () { null };
</script>