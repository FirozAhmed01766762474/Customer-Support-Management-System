@model IEnumerable<XFLCSMS.Models.Brocarage.Brokerage>

            <!-- Main content -->

            <section class="content">

                <!-- Default box -->
                <div class="card">
                    <div class="card-header">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-6">
                                <h4>Brokerage House List</h4>
                            </div>
                            <div class="col-md-6 text-right">
                                <div class="card-tools">
                                    <button type="button" class="btn btn-tool" data-card-widget="collapse" title="Collapse">
                                        <i class="fas fa-minus"></i>
                                    </button>
                                    <button type="button" class="btn btn-tool" data-card-widget="remove" title="Remove">
                                        <i class="fas fa-times"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    </div>
                    <div class="card-body">
                        <a class="btn btn-outline-success" asp-controller="Admin" asp-action="CreateBrocarage">
                            <i class="fas fa-plus"></i> Create
                        </a>
                    </div>
                    <div class="card-body p-0">
                        <table class="table table-striped projects" id="issuesTable">
                            <thead>
                                <tr>
                                    <th style="width:5%">
                                        BrocarageId
                                    </th>
                                    <th style="width: 30%">
                                        BrokerageHouseName
                                    </th>
                                    <th style="width: 15%">
                                        BrokerageHouseAcronym
                                    </th>
                                    <th style="width: 30%; text-align: center;">
                                        Modify
                                    </th>


                                </tr>
                            </thead>

                            <tbody>
                                @foreach (var item in Model)
                                {


                                    <tr id="row-@item.BrokerageId">
                                        <!-- Your table cells -->
                                        <td>@Html.DisplayFor(modelItem => item.BrokerageId)</td>
                                        <td>@Html.DisplayFor(modelItem => item.BrokerageHouseName)</td>
                                        <td>@Html.DisplayFor(modelItem => item.BrokerageHouseAcronym)</td>
                                        <td class="project-actions text-right">
                                            <a class="btn btn-outline-primary btn-sm" asp-controller="Admin" asp-action="ViewBrocarageHouse" asp-route-id="@item.BrokerageId">
                                                <i class="fas fa-folder"></i> View
                                            </a>
                                            <a class="btn btn-outline-info btn-sm" asp-controller="Admin" asp-action="EditBrocarage" asp-route-id="@item.BrokerageId">
                                                <i class="fas fa-pencil-alt"></i> Edit
                                            </a>
                                <a class="btn btn-outline-danger btn-sm" asp-controller="Admin" asp-action="DeleteBrocarage" asp-route-id="@item.BrokerageId">
                                                <i class="fas fa-trash-alt"></i>
                                                Delete
                                           </a>
                                        </td>
                                   </tr>
    <!-- Additional rows go here -->



                                }


                            </tbody>
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->

            </section>
            <!-- /.content -->
      @*   </div> *@
        <!-- /.content-wrapper -->






    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>

    <!-- Your JavaScript/jQuery code -->
    

<script type="text/javascript">
    // Define a function named preventback
    function preventback() {
        // Forward the browser's history by one step, effectively preventing the user from going back
        window.history.forward();
    }

    // Set a timeout to call the preventback function immediately after the script is executed
    setTimeout("preventback()", 0);

    // When the window loads, assign a null function to the onload event to effectively do nothing
    window.onload = function () { null };
</script>

