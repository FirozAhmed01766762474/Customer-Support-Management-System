@model XFLCSMS.Models.Brocarage.Brokerage;


<!-- Main content -->
<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- left column -->
            <div class="col-md-6">
                <!-- general form elements -->
                <div class="card card-primary">
                    <!-- /.card-header -->
                    <!-- form start -->
                    <form asp-controller="Admin" asp-action="UpdateBrocarage" method="post">
                        <div class="card-body">
                            <div class="form-group">
                                <label for="exampleInputEmail1">BrokerageId</label>
                                <input type="text" asp-for="@Model.BrokerageId" value="@Model.BrokerageId" class="form-control" id="exampleInputEmail1" placeholder="BrokerageId" readonly>
                            </div>

                            <div class="form-group">
                                <label for="exampleInputEmail1">BrokerageHouseName</label>
                                <input type="text" asp-for ="@Model.BrokerageHouseName" value="@Model.BrokerageHouseName" class="form-control" id="exampleInputEmail1" placeholder="BrokerageHouseName" >
                            </div>

                            <div class="form-group">
                                <label for="exampleInputEmail1">BrokerageHouseAcronym</label>
                                <input type="text" class="form-control" asp-for="@Model.BrokerageHouseAcronym" value="@Model.BrokerageHouseAcronym" id="exampleInputEmail1" placeholder="BrokerageHouseAcronym">
                            </div>

                            <div class="form-group">
                                <input type="submit" value="Update Brocarage" class="btn btn-primary" />
                            </div>



                        </div>
                        <!-- /.card-body -->
                    </form>
                </div>
                <!-- /.card -->
            </div>
            <!--/.col (left) -->
            <!-- right column -->
            <!--/.col (right) -->
        </div>
        <!-- /.row -->
        <!-- /.card-body -->

    </div>



    <!-- /.container-fluid -->
</section>
            <!-- /.content -->
     

<script type="text/javascript">
    // Define a function named preventback
    function preventback() {
        // Forward the browser's history by one step, effectively preventing the user from going back
        window.history.forward();
    }

    // Set a timeout to call the preventback function immediately after the script is executed
    setTimeout("preventback()", 0);

    // When the window loads, assign a null function to the onload event to effectively do nothing
    window.onload = function () { null };
</script>