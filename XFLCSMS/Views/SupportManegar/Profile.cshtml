                            <div class="form-group">
                                <label for="exampleInputEmail1">Employee Id</label>
                                <input type="text" value="@Model.EmployeeId" class="form-control" id="exampleInputEmail1" placeholder="Employee Id" readonly>
                            </div>
                            <div class="form-group">
